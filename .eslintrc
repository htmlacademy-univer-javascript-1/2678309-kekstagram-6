{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es2017": true,
    "browser": true
  },
  "globals": {
    "_": "readonly",
    "L": "readonly",
    "noUiSlider": "readonly",
    "Pristine": "readonly"
  },
  "rules": {
    "quotes": [1, "single"],
    "no-unused-vars": ["warn", {
      "vars": "all",
      "args": "after-used",
      "ignoreRestSiblings": true,
      "varsIgnorePattern": "^_|^photos$"
    }],
    "no-console": "warn"
  },
  "extends": [
    "htmlacademy/vanilla"
  ]
}